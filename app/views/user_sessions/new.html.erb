<%= form_with url: login_path do |f| %>
  
  <div>
    <%= f.text_field :username %>
    <%= f.label :username %>
  </div>
  <div>
    <%= f.email_field :email %>
    <%= f.label :email %>
  </div>
  <div>
    <%= f.password_field :password %>
    <%= f.label :password %>
  </div>
  <div>
    <%= f.password_field :password_confirmation %>
    <%= f.label :password_confirmation %>
  </div>
  <%= f.submit 'ログイン' %>
<% end %>